using UnityEngine;

[CreateAssetMenu(fileName = "NewSoundEmitterPool", menuName = "Pool/SoundEmitter Pool")]
public class EnvPatchPoolSO : ComponentPoolVarianceSO<Patch>
{
    [SerializeField] private EnvFactorySO envFactory;

    public override IFactoryVariance<Patch> Factory
    {
        get
        {
            return envFactory;
        }
        set
        {
            envFactory = value as EnvFactorySO;
        }
    }
}